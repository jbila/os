	<queryString>
		<![CDATA[SELECT c.nome Cliente,p.sub_total,p.total,
	p.created,u.username
FROM swing_simafa.tbl_pedido p
INNER JOIN swing_simafa.tbl_cliente c ON 
	 p.id_cliente = c.id
	 inner join swing_simafa.tbl_utilizador  u on p.id_utilizador=u.id
	 WHERE p.created between $P{DATA_INICIO} and 
	 $P{DATA_FINAL}]]>
	</queryString>
